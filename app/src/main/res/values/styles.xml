<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FFFF</item>
        <item name="colorPrimaryDark">#5B5F72</item>
        <item name="colorAccent">#BAD1628A</item>

    </style>

    <style name="listItem" parent="Widget.AppCompat.TextView.SpinnerItem">
        <item name="android:background">@drawable/some_rectangle_form</item>
        <item name="android:textColor" >@color/colorPrimary</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:layout_margin">5dp</item>
        <!--<item name="android:ed">false</item>-->
    </style>
    
    
    <style name="EntryButton" parent="TextAppearance.AppCompat.Button">
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:background">@color/colorPrimaryDark</item>
        
    </style>

    <!--<style name="ItemRow"/>-->

    <!--<style name="ItemRow.Selectable">-->
        <!--<item name="android:clickable">true</item>-->
    <!--</style>-->
</resources>
